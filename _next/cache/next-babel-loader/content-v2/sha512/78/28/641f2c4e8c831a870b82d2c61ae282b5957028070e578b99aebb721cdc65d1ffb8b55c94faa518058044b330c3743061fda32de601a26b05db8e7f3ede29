\n                                                                                <option style={{ display: 'none' }} value=''></option>\n                                                                                {\n                                                                                    webPages.length > 0 && webPages.map((page, index) => {\n                                                                                        return (\n                                                                                            <option value={page.gid} key={index}> { page.code } </option>\n                                                                                        )\n                                                                                    })\n                                                                                }\n                                                                            </Select>\n                                                                        </FormControl>\n                                                                    </Grid>\n                                                                </Grid>\n                                                            }\n                                                            {\n                                                                footerItems[activeRowStep - 1].items[index].value[i].type === 'address' &&\n                                                                <Grid container={true}\n                                                                      justify={'flex-start'}\n                                                                      spacing={3}\n                                                                >\n                                                                    <Grid item xs={6}>\n                                                                        <TextField\n                             