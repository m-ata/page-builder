={() => setOptimizeImages(!isOptimizeImages)}\n                                        color=\"primary\"\n                                    />\n                                }\n                                label=\"Optimize Images\"\n                            />\n                            {router?.query?.sliderOnly && (\n                                <>\n                                    <FormControlLabel\n                                        className={classes.labelFont}\n                                        control={\n                                            <Checkbox\n                                                size={'small'}\n                                                checked={isActive}\n                                                onChange={() => {\n                                                    setIsActive(!isActive)\n                                                    Patch({\n                                                        // update slider\n                                                        apiUrl: GENERAL_SETTINGS.OREST_URL,\n                                                        endpoint: OREST_ENDPOINT.HCMITEMSLD,\n                                                        token: authToken,\n                                                        gid: sliderGID,\n                                                        params: {\n                                                            hotelrefno: Number(companyId),\n                                                        },\n                                                        data: {\n                                                            isactive: !isActive,\n                                                        },\n                                                    }).then((res) => {\n                                                        if (res.status !== 200) {\n                                                            const retErr 