ink'}\n                                                fullWidth\n                                                value={ctaLink}\n                                                error={ctaLink ? !validator.isURL(ctaLink) : false}\n                                                onChange={(e) => setCtaLink(e.target.value)}\n                                                style={{ marginTop: 8 }}\n                                                disabled={state.langCode !== state.defaultLang}\n                                                helperText={\n                                                    ctaLink && (\n                                                        <Typography\n                                                            variant=\"caption\"\n                                                            className={classes.alignText}\n                                                            display=\"block\"\n                                                        >\n                                                            {`${ctaLink?.length} < 100`}\n                                                        </Typography>\n                                                    )\n                                                }\n                                                inputProps={{ maxLength: 99 }}\n                                            />\n                                        )}\n                                        {ctaLinkType === 'internal' && (\n                                            <FormControl\n                                                variant=\"outlined\"\n                                                size={'small'}\n                                                style={{ marginTop: 8 }}\n                                                fullWidth\n                                                disabled={state?.langCode !== state?.defaultLang}\n                                            >\n            