                                                          }\n                                                                            {\n                                                                                value.type === 'heading' &&\n                                                                                <h5 className={classes.text}>\n                                                                                    {\n                                                                                        value.value\n                                                                                    }\n                                                                                    <span></span>\n                                                                                </h5>\n                                                                            }\n                                                                            {\n                                                                                value.type === 'paragraph' &&\n                                                                                <p className={classes.text}>\n                                                                                    {value.value}\n                                                                                </p>\n                                                                            }\n                                                                            {\n                                                                                value.type === 'address' &&\n                                                                                <a\n                                                                                    className={classes.link}\n                                                                                    href={value.value.value}\n                                            